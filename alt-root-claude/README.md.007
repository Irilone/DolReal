│   ├── modules/            # ES6+ modules
│   │   ├── gemini-api-module.js
│   │   ├── utility.js
│   │   └── validation.js
│   ├── index.html          # Main UI
│   ├── scripts.js          # Application logic
│   ├── style.css           # Styling
│   ├── README.md           # Orchestration docs
