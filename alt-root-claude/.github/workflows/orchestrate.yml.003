      - name: Setup Bun
        uses: oven-sh/setup-bun@v1
        with:
          bun-version: ${{ env.BUN_VERSION }}

      - name: Install dependencies
        run: bun install

      - name: Run Gemini Ultra Research
        env:
          GEMINI_API_KEY: ${{ secrets.GEMINI_API_KEY }}
        run: make gemini-ultra

