{
  "name": "dol-2025",
  "version": "2.0.0",
  "description": "Dagar om Lagar 2025 - Multi-agent orchestration streaming platform",
  "private": true,
  "type": "module",
  "scripts": {
    "orchestrate": "make all",
    "gem:research": "bun run scripts/gem.ts --model gemini-2.5-pro-ultra --input prompts/1_gemini_ultra_research.md --output artifacts/1_gemini_ultra_research.json",
    "gpt:arch": "bun run scripts/openai.ts --model gpt-5-codex --input /tmp/gpt5-input.md --output artifacts/2_gpt5_codex_architecture.json",
    "claude:frontend": "bun run scripts/anthropic.ts --model claude-sonnet-4-5 --input /tmp/claude-frontend-input.md --output artifacts/3a_claude_frontend_output.json",
    "claude:backend": "bun run scripts/anthropic.ts --model claude-sonnet-4-5 --input /tmp/claude-backend-input.md --output artifacts/3b_claude_backend_output.json",
    "gem:integrate": "bun run scripts/gem.ts --model gemini-2.5-pro --input /tmp/gemini-cli-input.md --output artifacts/4_gemini_cli_final.json",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "clean": "rm -rf .next out dist artifacts/* plans/* releases/* logs/*"
  },
