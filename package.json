{
  "name": "dol-2025",
  "version": "2.0.0",
  "description": "Dagar om Lagar 2025 - Multi-agent orchestration streaming platform",
  "private": true,
  "type": "module",
  "scripts": {
    "orchestrate": "make all",
    "gem:research": "bun run scripts/gem.ts --model gemini-2.5-pro-ultra --input prompts/1_gemini_ultra_research.md --output artifacts/1_gemini_ultra_research.json",
    "gpt:arch": "bun run scripts/openai.ts --model gpt-5-codex --input /tmp/gpt5-input.md --output artifacts/2_gpt5_codex_architecture.json",
    "claude:frontend": "bun run scripts/anthropic.ts --model claude-sonnet-4-5 --input /tmp/claude-frontend-input.md --output artifacts/3a_claude_frontend_output.json",
    "claude:backend": "bun run scripts/anthropic.ts --model claude-sonnet-4-5 --input /tmp/claude-backend-input.md --output artifacts/3b_claude_backend_output.json",
    "gem:integrate": "bun run scripts/gem.ts --model gemini-2.5-pro --input /tmp/gemini-cli-input.md --output artifacts/4_gemini_cli_final.json",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "clean": "rm -rf .next out dist artifacts/* plans/* releases/* logs/*"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "googleapis": "^134.0.0",
    "i18next": "^23.7.0",
    "next": "^15.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.0.0",
    "tailwind-merge": "^3.3.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "streaming",
    "youtube",
    "infranodus",
    "multi-agent",
    "orchestration",
    "ai"
  ],
  "author": "DoL 2025 Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dorian/dol-2025.git"
  }
}
